<launch>
  <!-- MuJoCo核心节点（去掉scripts/前缀） -->
  <node 
    name="mujoco_core" 
    pkg="mujoco_ros" 
    type="main.py"
    output="screen"
    args="visualize /home/lan/mujoco_models/anybotics_anymal_b/anymal_b.xml --ros"
  />

  <!-- 控制指令发布者（去掉scripts/前缀） -->
  <node 
    name="mujoco_ctrl_publisher" 
    pkg="mujoco_ros" 
    type="publisher.py"
    output="screen"
  />

  <!-- 状态订阅者（去掉scripts/前缀） -->
  <node 
    name="mujoco_state_subscriber" 
    pkg="mujoco_ros" 
    type="subscriber.py"
    output="screen"
  />
</launch>